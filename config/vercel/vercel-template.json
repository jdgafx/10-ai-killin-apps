{
  "$schema": "https://openapis.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "functions": {},
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_MINIMAX_API_KEY": "@minimax_api_key",
    "VITE_DEEPSEEK_API_KEY": "@deepseek_api_key",
    "VITE_GITHUB_COPILOT_TOKEN": "@github_copilot_token",
    "VITE_GEMINI_CLIENT_ID": "@gemini_client_id",
    "VITE_ENABLE_RAG": "true",
    "VITE_ENABLE_VOICE": "true",
    "VITE_DEFAULT_PROVIDER": "minimax",
    "NODE_VERSION": "18"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  }
}
